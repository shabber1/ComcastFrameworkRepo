<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" parallel="tests" name="Suite">

	<groups>
		<run>
			<include name="regressionTest" />
		</run>
	</groups>
	
	<test parallel="tests" name="TestRunner-1">
		<parameter name="BROWSER" value="firefox" />
		<classes>
			<class name="com.comcast.crm.orgtest.OrgModule" />
		</classes>
	</test> <!-- Test -->
	
	<test parallel="tests" name="TestRunner-2">
		<parameter name="BROWSER" value="chrome" />
		<classes>
			<class name="com.comcast.crm.contacttest.ContactModule" />
		</classes>
	</test> <!-- Test -->
	
</suite> <!-- Suite -->
